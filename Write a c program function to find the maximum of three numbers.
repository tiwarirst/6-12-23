#include <stdio.h>
int fan(int a,int b,int c)
{
    if(a>b)
    {
        if(a>c)
        printf("maximum of three number is%d",a);
        else
        printf("maximum of three number is %d",c);
    }
    else
    { if(b>c)
    printf("maximum of three number is %d",b);
    else
    printf("maximum of three number is %d",c);
    }
}
int main()
{
    int a[3];
    for (int i=0;i<3;i++)
    {printf("enter the no:");
        scanf("%d",&a[i]);}
    fan(a[0],a[1],a[2]);
    return 0;
}
